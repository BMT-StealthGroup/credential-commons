{
  "$id": "http://civic.com/uca/schemas/identity.name.json",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "first": {
      "$id": "#/properties/first",
      "$ref": "identity.name.first.json#/properties/identity.name.first"
    },
    "middle": {
      "$id": "#/properties/middle",
      "$ref": "identity.name.middle.json#/properties/identity.name.middle"
    },
    "last": {
      "$id": "#/properties/last",
      "$ref": "identity.name.last.json#/properties/identity.name.last"
    }
  },
  "toString": "${first} ${middle} ${last}",
  "fromString": "(?:<first>\\S+) (?:<middle>\\S+) (?:<last>\\S+)",
  "CredentialItem": true
}
